<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.pavilion.click2wish.cakes">

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="65dp"
        android:layout_height="59dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="18dp"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="63dp"
        android:layout_height="58dp"
        android:layout_alignLeft="@+id/iv3"
        android:layout_alignStart="@+id/iv3"
        android:layout_below="@+id/iv1"
        android:layout_marginTop="27dp"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="100dp" />

    <CheckBox
        android:id="@+id/c2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/c1"
        android:layout_alignStart="@+id/c1"
        android:layout_alignTop="@+id/iv2"
        android:layout_marginTop="12dp"
        android:text="Vanilla Cake"
        tools:layout_editor_absoluteX="194dp"
        tools:layout_editor_absoluteY="119dp" />

    <Button
        android:id="@+id/b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="order"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="447dp" />

    <CheckBox
        android:id="@+id/c1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv1"
        android:layout_alignLeft="@+id/b"
        android:layout_alignStart="@+id/b"
        android:layout_marginBottom="13dp"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:text="Red Velvet Cake"
        tools:layout_editor_absoluteX="194dp"
        tools:layout_editor_absoluteY="34dp" />

    <CheckBox
        android:id="@+id/c3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/c2"
        android:layout_alignStart="@+id/c2"
        android:layout_alignTop="@+id/iv3"
        android:layout_marginTop="14dp"
        android:text="Pineapple Cake"
        tools:layout_editor_absoluteX="194dp"
        tools:layout_editor_absoluteY="212dp" />

    <CheckBox
        android:id="@+id/c4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv4"
        android:layout_alignEnd="@+id/c3"
        android:layout_alignRight="@+id/c3"
        android:layout_marginBottom="13dp"
        android:text="Chocolate Cake"
        tools:layout_editor_absoluteX="194dp"
        tools:layout_editor_absoluteY="299dp" />

    <CheckBox
        android:id="@+id/c5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/b"
        android:layout_alignLeft="@+id/c4"
        android:layout_alignStart="@+id/c4"
        android:layout_marginBottom="31dp"
        android:text="Butterscotch Cake"
        tools:layout_editor_absoluteX="194dp"
        tools:layout_editor_absoluteY="381dp" />

    <ImageView
        android:id="@+id/iv3"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_above="@+id/iv4"
        android:layout_alignEnd="@+id/iv1"
        android:layout_alignRight="@+id/iv1"
        android:layout_marginBottom="24dp"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="191dp" />

    <ImageView
        android:id="@+id/iv4"
        android:layout_width="63dp"
        android:layout_height="62dp"
        android:layout_above="@+id/iv5"
        android:layout_alignLeft="@+id/iv3"
        android:layout_alignStart="@+id/iv3"
        android:layout_marginBottom="40dp"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="279dp" />

    <ImageView
        android:id="@+id/iv5"
        android:layout_width="63dp"
        android:layout_height="58dp"
        android:layout_above="@+id/b"
        android:layout_alignLeft="@+id/iv4"
        android:layout_alignStart="@+id/iv4"
        android:layout_marginBottom="19dp"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="366dp" />


</RelativeLayout>
